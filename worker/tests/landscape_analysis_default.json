            },
            {
                "regex": "/(accounts?|profiles?|users?)",
                "priority": 88
            },
            {
                "regex": "/(register|registration)",
                "priority": 88
            },
            {
                "regex": "/(manage|managing|anmelden|portal|dashboard)",
                "priority": 87
            },
            {
                "regex": "/connect(ing|ion|ed)?",
