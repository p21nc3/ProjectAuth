            {
                "regex": "[^a-z](accounts?|profiles?|users?)[^a-z]",
                "priority": 78
            },
            {
                "regex": "[^a-z](register|registration)[^a-z]",
                "priority": 78
            },
            {
                "regex": "[^a-z](manage|managing|anmelden|portal|dashboard)[^a-z]",
                "priority": 77
            },
            {
                "regex": "[^a-z]connect(ing|ion|ed)?[^a-z]",
                "priority": 77
            }
